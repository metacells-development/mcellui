{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@/components/ui/*": ["../../packages/registry/ui/*"],
      "@/components/blocks/*": ["../../packages/registry/blocks/*"],
      "@/components/screens/*": ["../../packages/registry/screens/*"],
      "@/components/*": ["./components/*"],
      "@/lib/*": ["./lib/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
