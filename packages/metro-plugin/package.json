{
  "name": "@metacells/mcellui-metro-plugin",
  "version": "0.1.2",
  "description": "Metro plugin for mcellui auto-config discovery",
  "author": "metacells",
  "license": "MIT",
  "homepage": "https://mcellui.dev",
  "repository": {
